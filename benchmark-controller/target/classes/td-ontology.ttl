          rdfs:label "has cause"^^xsd:string ;
          
          rdfs:comment "Indicates that a Disruptive Impact is caused by an Event."^^xsd:string ;
          
          rdfs:range <http://purl.org/NET/c4dm/event.owl#Event> ;
          
          rdfs:domain :DisruptiveImpact .



###  http://purl.org/td/transportdisruption#hasPlan

:hasPlan rdf:type owl:ObjectProperty ;
         
         rdfs:label "has plan"^^xsd:string ;
         
         rdfs:comment "Indicates a Plan that a foaf:Agent has."^^xsd:string ;
         
         rdfs:range :Plan ;
         
         rdfs:domain <http://xmlns.com/foaf/0.1/Agent> .



###  http://purl.org/td/transportdisruption#impactsOn

:impactsOn rdf:type owl:ObjectProperty ;
           
           rdfs:label "impacts on"^^xsd:string ;
           
           rdfs:comment "Indicates a Plan that a Disruptive Impact affects."^^xsd:string ;
           
           rdfs:domain :DisruptiveImpact ;
           
           rdfs:range :Plan .



###  http://purl.org/td/transportdisruption#relevantTo

